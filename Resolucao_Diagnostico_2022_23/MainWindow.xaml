<Window x:Class="Resolucao_Diagnostico_2022_23.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Resolucao_Diagnostico_2022_23"
        mc:Ignorable="d"
        Title="Portaria" Height="600" Width="800">
    <Grid>
        <TextBox Name="tbNProcesso" HorizontalAlignment="Left" 
                 VerticalAlignment="Top" Margin="10,10"
                 Width="100" Height="40"/>
        <Button Name="btProcessaMovimento" HorizontalAlignment="Left"
                VerticalAlignment="Top" Margin="120,10" Content="Ok" 
                Width="60" Height="40" Click="btProcessaMovimento_Click"/>
        <Image Name="imgFoto" Margin="10,50" HorizontalAlignment="Left"
               VerticalAlignment="Top" Width="100" Height="150"/>
        <Label Name="lbMensagem" Margin="10,210" HorizontalAlignment="Left"
               VerticalAlignment="Top" Width="200" Height="40"/>
        <Button Name="btListaMovimentos" HorizontalAlignment="Left"
                VerticalAlignment="Top" Margin="10,260" 
                Content="Lista movimentos" Height="40" 
                Click="btListaMovimentos_Click" />
    </Grid>
</Window>
